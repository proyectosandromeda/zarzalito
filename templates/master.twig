		<script type='text/javascript'>
			var BaseUrl = {
"ajaxurl": "{{ base_path() }}",
"rol": {{ session.rol }}
}

var sessionTimeout = "2";

function DisplaySessionTimeout() {
sessionTimeout = sessionTimeout - 1;

if (sessionTimeout >= 0) {
window.setTimeout("DisplaySessionTimeout()", 1800000);

} else {
location.href = BaseUrl.ajaxurl + "/login/logout"
}
}

DisplaySessionTimeout()
