		<!-- Preloader - style you can find in spinners.css -->
		<!-- ============================================================== -->
			<div class="preloader"> <div style="position: absolute;
																																																										width: 90px;
																																																										height: 90px;
																																																										left: calc(50% - 90px/2 + 79px);
																																																										top: calc(50% - 90px/2);
																																																										background: #FFFFFF;
																																																										border-radius: 5px;">
				<div class="spinner-border spinner-border-lg text-primary" role="status" style="margin: calc(50% / 2) calc(50% / 2);">
					<span class="visually-hidden">Cargando...</span>
				</div>
			</div>
		</div>


